{
  "name": "squelch",
  "productName": "Squelch",
  "version": "0.0.0",
  "description": "An IRC client",
  "private": "true",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean": "rimraf release/",
    "eslint": "eslint --ext .js,.jsx app main.js",
    "lint": "npm run eslint",
    "prebuild": "npm run clean && npm run lint",
    "build": "electron-packager . Squelch --asar --version=0.28.3 --all --out release --prune --ignore=\"/test($|/)|/tools($|/)|/release($|/)\"",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squelch-irc/squelch.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/squelch-irc/squelch/issues"
  },
  "homepage": "https://github.com/squelch-irc/squelch#readme",
  "devDependencies": {
    "babel-eslint": "^3.1.24",
    "electron-packager": "^5.0.0",
    "electron-prebuilt": "^0.28.3",
    "electron-rebuild": "^0.2.1",
    "eslint": "^1.1.0",
    "eslint-config-kellyirc": "^2.0.0",
    "eslint-plugin-react": "^3.0.0",
    "minimist": "^1.1.1",
    "rimraf": "^2.4.1"
  },
  "dependencies": {
    "alt": "^0.17.1",
    "babel-core": "^5.6.15",
    "bootstrap": "^3.3.5",
    "classnames": "^2.1.3",
    "electron-debug": "^0.1.0",
    "eventemitter2": "^0.4.14",
    "fs-extra": "^0.20.1",
    "less-hot": "^1.0.0",
    "lodash": "^3.10.0",
    "moment": "^2.10.3",
    "q": "^1.4.1",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-treeview": "^0.4.0",
    "squelch-client": "^0.10.1"
  }
}
