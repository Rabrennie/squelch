{
  "name": "squelch",
  "productName": "Squelch",
  "version": "0.0.0",
  "description": "An IRC client",
  "private": "true",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean": "rimraf release/",
    "lint": "eslint --ext .js,.jsx app main.js menu.js",
    "prebuild": "npm run clean && npm run lint",
    "build": "electron-packager . --platform=darwin --arch=x64 release --prune --ignore='^/test$'",
    "pretest": "npm run lint",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squelch-irc/squelch.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/squelch-irc/squelch/issues"
  },
  "homepage": "https://github.com/squelch-irc/squelch#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-eslint": "^5.0.0-beta6",
    "electron-packager": "^7.0.3",
    "electron-prebuilt": "^1.2.2",
    "electron-rebuild": "^1.0.2",
    "eslint": "~2.2.0",
    "eslint-config-kellyirc": "^3.1.0",
    "eslint-plugin-react": "^4.1.0",
    "rimraf": "^2.4.1"
  },
  "dependencies": {
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "bluebird": "^3.3.4",
    "catw": "^1.0.1",
    "classnames": "^2.1.3",
    "concat-stream": "^1.5.1",
    "electron-debug": "^0.5.2",
    "event-kit": "^2.0.0",
    "freezer-js": "^0.9.4",
    "fs-extra": "^0.26.5",
    "less-hot": "^1.0.0",
    "lodash": "^4.5.1",
    "moment": "^2.10.3",
    "react": "^0.14.1",
    "react-dom": "^0.14.6",
    "react-router": "^2.0.0-rc4",
    "react-treeview": "^0.4.3",
    "require-dir": "^0.3.0",
    "squelch-base-theme": "0.0.6",
    "squelch-client": "^0.13.1",
    "squelch-nick-hash": "^1.0.1",
    "string-args": "^1.0.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  }
}
