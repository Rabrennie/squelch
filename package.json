{
  "name": "squelch-irc",
  "version": "0.0.0",
  "description": "An IRC client",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "node scripts/build-release.js",
    "build-win32": "npm run build -- --platform win32 --arch ia32",
    "build-win64": "npm run build -- --platform win32 --arch x64",
    "build-osx32": "npm run build -- --platform darwin --arch ia32",
    "build-osx64": "npm run build -- --platform darwin --arch x64",
    "build-linux32": "npm run build -- --platform linux --arch ia32",
    "build-linux64": "npm run build -- --platform linux --arch x64",
    "build-linux": "npm run build-linux32 && npm run build-linux64",
    "build-win": "npm run build-win32 && npm run build-win32",
    "build-osx": "npm run build-osx32 && npm run build-osx32"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/squelch-irc/squelch.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/squelch-irc/squelch/issues"
  },
  "homepage": "https://github.com/squelch-irc/squelch#readme",
  "devDependencies": {
    "babel-eslint": "^3.1.14",
    "electron-packager": "^4.1.0",
    "electron-prebuilt": "^0.28.3",
    "electron-rebuild": "^0.2.1",
    "eslint": "^0.22.1",
    "eslint-plugin-react": "^2.5.0",
    "minimist": "^1.1.1"
  },
  "dependencies": {
    "babel": "^5.6.9",
    "debug": "^2.2.0",
    "electron-debug": "^0.1.0",
    "flux": "^2.0.3",
    "keymirror": "^0.1.1",
    "lodash": "^3.9.3",
    "object-assign": "^2.0.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3"
  }
}
